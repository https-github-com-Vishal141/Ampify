<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="530.0" prefWidth="718.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Player.AudioPlayer">
   <children>
      <Pane fx:id="pane" layoutX="140.0" layoutY="67.0" prefHeight="317.0" prefWidth="382.0">
         <children>
            <Label fx:id="lyrics" layoutX="41.0" layoutY="71.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children></Pane>
      <Button layoutY="1.0" mnemonicParsing="false" onAction="#exit" text="exit" />
      <HBox alignment="CENTER" layoutX="-2.0" layoutY="422.0" prefHeight="106.0" prefWidth="718.0">
         <children>
            <Pane prefHeight="112.0" prefWidth="709.0">
               <children>
                  <Slider fx:id="slider" layoutX="98.0" layoutY="74.0" prefHeight="14.0" prefWidth="544.0" />
                  <Button fx:id="play" layoutX="296.0" layoutY="28.0" mnemonicParsing="false" onAction="#play" text="pause" />
                  <Button layoutX="363.0" layoutY="28.0" mnemonicParsing="false" onAction="#next" text="next" />
                  <Button layoutX="222.0" layoutY="28.0" mnemonicParsing="false" onAction="#previous" text="previous" />
                  <Label fx:id="cTime" layoutX="54.0" layoutY="73.0" text="00:00" />
                  <Label fx:id="tTime" layoutX="652.0" layoutY="73.0" text="00:00" />
                  <Button layoutX="419.0" layoutY="28.0" mnemonicParsing="false" onAction="#repeat" text="repeat" />
                  <Button layoutX="485.0" layoutY="28.0" mnemonicParsing="false" onAction="#shuffle" text="shuffle" />
                  <Button layoutX="626.0" layoutY="16.0" mnemonicParsing="false" onAction="#gotoEqualizer" text="EQ" />
               </children>
            </Pane>
         </children>
      </HBox>
      <Label fx:id="songName" layoutX="171.0" layoutY="34.0" text="Name" />
      <ComboBox fx:id="queue" layoutX="553.0" layoutY="75.0" onAction="#playQueue" onSwipeRight="#swipe" prefWidth="150.0" promptText="Queue" />
      <Button fx:id="like" layoutX="32.0" layoutY="193.0" mnemonicParsing="false" onAction="#like" text="Like" />
      <Button fx:id="dislike" layoutX="31.0" layoutY="239.0" mnemonicParsing="false" onAction="#DisLike" prefHeight="18.0" prefWidth="62.0" text="Dislike" />
      <Button fx:id="download" layoutX="31.0" layoutY="283.0" mnemonicParsing="false" onAction="#download" prefHeight="26.0" prefWidth="75.0" text="Download" />
   </children>
</AnchorPane>
