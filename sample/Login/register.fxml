<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="478.0" prefWidth="677.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Login.RegisterController">
   <children>
      <Label layoutX="60.0" layoutY="32.0" text="Sign Up">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="email" layoutX="60.0" layoutY="87.0" prefHeight="26.0" prefWidth="208.0" promptText="Email" />
      <TextField fx:id="username" layoutX="60.0" layoutY="138.0" prefHeight="26.0" prefWidth="208.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="60.0" layoutY="187.0" prefHeight="26.0" prefWidth="208.0" promptText="Password" />
      <TextField fx:id="cPassword" layoutX="60.0" layoutY="238.0" prefHeight="26.0" prefWidth="208.0" promptText="Confirm Password" />
      <Button fx:id="register" layoutX="216.0" layoutY="291.0" mnemonicParsing="false" onAction="#onRegister" text="Register" />
      <Button fx:id="login" layoutX="60.0" layoutY="351.0" mnemonicParsing="false" onAction="#onLogin" text="Log In" />
      <ListView fx:id="languages" layoutX="339.0" layoutY="38.0" prefHeight="200.0" prefWidth="135.0" />
      <ListView fx:id="generes" layoutX="339.0" layoutY="277.0" prefHeight="200.0" prefWidth="135.0" />
      <ListView fx:id="artists" layoutX="528.0" layoutY="39.0" prefHeight="194.0" prefWidth="135.0" />
      <Label layoutX="339.0" layoutY="14.0" text="Languages" />
      <Label layoutX="339.0" layoutY="255.0" text="Generes" />
      <Label layoutX="528.0" layoutY="14.0" text="Artists" />
   </children>
</AnchorPane>
